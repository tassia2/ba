<Param>
  <Mesh>
    <Filename>channel.inp</Filename><!-- filename of mesh-->
    <InitialRefLevel>2</InitialRefLevel><!-- initial mesh refinement level-->
    <FinalRefLevel>2</FinalRefLevel><!-- final mesh refinement level (should be equal to initial ref level)-->
  </Mesh>
  <FESpace>
    <VelocityDegree>2</VelocityDegree><!-- Polynomial degree of velocity FE-->
    <PressureDegree>1</PressureDegree><!-- Polynomial degree of pressure FE-->
  </FESpace>
  <Equation>
    <nu>1e-3</nu><!-- viscosity-->
    <Cv>1</Cv><!-- SUPG stabilization parameter-->
    <Cp>.1</Cp><!-- PSPG stabilization parameters-->
    <WithConvection>1</WithConvection><!-- 1: with convection(Navier-Stokes), 0: no convection (Stokes)-->
    <WithStabilization>0</WithStabilization><!-- 1: use SUPG and PSPG, 0: no stabilization-->
    <PressureZeroAverage>0</PressureZeroAverage><!-- Force zero average pressure (only needed for pure Dirichlet BC on velocity)-->
  </Equation>
  <BoundaryConditions>
    <InflowVelocityX>1.0</InflowVelocityX><!-- Dirichlet BC for x-component of velocity-->
    <InflowRamp>0.1</InflowRamp><!-- Time period, fter wichich inflow and rotation reach their final value-->
    <RevolutionsPerSecond>10</RevolutionsPerSecond><!-- speed of rotating foil-->
    <CenterX>0.5</CenterX><!-- Center of foil (don't change)-->
    <CenterY>0.5</CenterY><!-- Center of foil (don't change)-->
    <MaterialNumbers><!-- Material numbers used to define BC-->
        <Inflow>11</Inflow><!-- inflow bc on left hand side -->
        <Outflow>12</Outflow><!-- free bc on right hand side -->
        <SlipX>13</SlipX><!-- top and bottom boundary --> 
        <Foil>21</Foil><!-- foil -->
    </MaterialNumbers>
  </BoundaryConditions>
  <TimeStepping>
    <T>1.0</T> <!-- Time period to be simulated-->
    <dt>0.01</dt><!-- time step size-->
    <BDFType>1</BDFType><!-- order of BDF time stepping method (1 or 2)-->
  </TimeStepping>
  <LinearSolver><!-- Linear Solver parameters-->
    <Name>GMRES</Name>
    <SizeBasis>200</SizeBasis>
    <Method>RightPreconditioning</Method>
    <MaxIterations>1000</MaxIterations>
    <AbsTolerance>1.0e-15</AbsTolerance>
    <RelTolerance>1.0e-10</RelTolerance>
    <DivTolerance>1.0e6</DivTolerance>
  </LinearSolver>
</Param>


                