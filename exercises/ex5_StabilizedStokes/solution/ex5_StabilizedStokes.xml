<Param>
  <Mesh>
    <Filename2>unit_square-2_tri.inp</Filename2>
    <Filename3>unit_cube.inp</Filename3>
    <InitialRefLevel>4</InitialRefLevel>
    <FinalRefLevel>7</FinalRefLevel>
  </Mesh>
  <FESpace>
    <VelocityDegree>1</VelocityDegree>
    <PressureDegree>1</PressureDegree>
  </FESpace>
  <Equation>
    <nu>1</nu>
    <MaxVelocity>1.</MaxVelocity>
    <PressureStabType>2</PressureStabType>
    <Gamma>.1</Gamma>
  </Equation>
  <NonlinearSolver>
    <MaximumIterations>15</MaximumIterations>
    <AbsoluteTolerance>1.e-15</AbsoluteTolerance>
    <RelativeTolerance>1.e-6</RelativeTolerance>
    <DivergenceLimit>1.e6</DivergenceLimit>
    <MaxArmijoIteration>0</MaxArmijoIteration>
    <ForcingStrategy>EisenstatWalker1</ForcingStrategy>
    <ConstantForcingTerm>1.e-4</ConstantForcingTerm>
    <InitialValueForcingTerm>1.e-3</InitialValueForcingTerm>
    <MaxValueForcingTerm>0.9</MaxValueForcingTerm>
    <GammaParameterEW2>0.9</GammaParameterEW2>
    <AlphaParameterEW2>2</AlphaParameterEW2>
  </NonlinearSolver>
  <LinearSolver>
    <Name>GMRES</Name>
    <SizeBasis>400</SizeBasis>
    <Method>RightPreconditioning</Method>
    <MaxIterations>1000</MaxIterations>
    <AbsTolerance>1.0e-15</AbsTolerance>
    <RelTolerance>1.0e-10</RelTolerance>
    <DivTolerance>1.0e6</DivTolerance>
  </LinearSolver>
</Param>
